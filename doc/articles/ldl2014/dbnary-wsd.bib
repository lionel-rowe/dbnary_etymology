@inproceedings{Boitet2010,
abstract = {After identifying distinctive characteristics of SLLPs (Special¬ized Languages for Linguistic Programming), such as their high-level data and control structures, the organization of lingware written in SLLPs as components (properties, models, rules, grammars, automata, dictionaries, raw or annotated corpora), and the often huge size and incremental character of some components, we introduce A. Colmerauer's Q-language, used to write transformational systems on chain-graphs of labelled trees known as Q-systems, and show its practical and theoretical power. In the context of ubilingual computing, we are reengineering existing SLLPs, to extend and scale them up for building new kinds of heterogeneous NLP systems. Using the example of the Q-language, we present some techniques to implement SLLPs so that the lingware has unbounded size, can be incrementally modi¬fied, Web-based cooperative lingware development is possible, and the computing model is also easy to extend.},
author = {Boitet, Christian and Cattan\'{e}o, David and Nguyen, Hong-Thai and Durand, Jean-Claude and Huynh, Cong-Phap},
booktitle = {SLE},
keywords = {linguistic programming,machine translation,nlp,q-language,q-systems,sllp,software engineering,specialized language for},
title = {{Reengineering Colmerauer ' s Q-language , a Specialized Language for Linguistic Programming}},
year = {2010}
}
